{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "npm run schema && npm run relay && npx ts-node -O '{\"module\": \"none\"}' generateBackEndJson.ts",
    "start": "npx expo start --tunnel",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "eject": "npx expo eject",
    "schema": "npx get-graphql-schema \"http://localhost:3030/api\" > ./schema.graphql",
    "relay": "npx relay-compiler",
    "fmt": "npx prettier --ignore-path .prettierignore --write .",
    "fmt:check": "npx prettier --ignore-path .prettierignore --check .",
    "tsc": "npx tsc",
    "lint": "npx eslint --ext .tsx --ext .ts src/",
    "lint:lazy": "npx eslint --debug $(git diff --name-only --diff-filter=d origin/main | grep -E '[jt]sx*$')",
    "lint:lazy-fix": "npx eslint --fix $(git diff --name-only --diff-filter=d origin/main | grep -E '[jt]sx*$')",
    "test": "npx jest --coverage=false",
    "test:coverage": "npx jest",
    "test:dev": "npx jest -o --watch --coverage=false"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "relay": {
    "artifactDirectory": "./src/__generated__",
    "language": "typescript",
    "schema": "./schema.graphql",
    "src": "./src"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "expo": "^45.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "lodash": "^4.17.21",
    "npm": "^8.3.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-screens": "~3.11.1",
    "react-relay": "^13.0.1",
    "relay-runtime": "^13.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@eslint/eslintrc": "^0.4.3",
    "@expo/ngrok": "^4.1.0",
    "@npmcli/fs": "^1.1.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.11",
    "@types/react-native": "~0.67.6",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-relay": "^13.0.1",
    "@types/relay-runtime": "^13.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-plugin-relay": "^13.0.1",
    "eslint": "^7.3.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-relay": "^1.8.3",
    "get-graphql-schema": "2.1.2",
    "graphql": "^15.5.0",
    "metro": "^0.68",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-devtools-core": "^4.23.0",
    "react-native-svg-transformer": "^1.0.0",
    "relay-compiler": "^13.0.1",
    "svgo": "2.8.0",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.4"
  },
  "private": true
}
